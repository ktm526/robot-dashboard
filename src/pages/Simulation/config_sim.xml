	<EntranceMaximumReservationNum  EntranceMaximumReservationNum="5"/> // 노드 대기 버퍼
	<MultiPathDistanceTolerance MultiPathDistanceTolerance="600"/> //
	//로봇들 초기 정보
</Simulation>